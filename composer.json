{
    "name": "drupal/cloudfront_purger",
    "type": "drupal-module",
    "description": "Provides a Purger for use with AWS CloudFront",
    "keywords": [
        "Drupal"
    ],
    "license": "GPL-2.0+",
    "homepage": "https://www.drupal.org/project/cloudfront_purger",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "http://drupal.org/project/issues/cloudfront_purger",
        "source": "http://github.com/previousnext/cloudfront_purger"
    },
    "require": {
        "aws/aws-sdk-php": "~3.0"
    },
    "require-dev": {
        "drupal/coder": "~8.0",
        "squizlabs/php_codesniffer": "~2.7"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "config": {
        "bin-dir": "bin/"
    }
}
